   "source": [
    "# LOAD pre-processed hospital and population data (if already available)\n",
    "FILENAME = 'ethiopia_49911-h-o.pickle'\n",
    "with open(PROCESSEDFILEPATH+'/'+FILENAME, 'rb') as handle:\n",
    "    hospitals,origins = pickle.load(handle)"
   ]
